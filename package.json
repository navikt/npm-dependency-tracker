{
    "name": "package-crawler-frontend",
    "version": "0.0.1",
    "npmClient": "yarn",
    "scripts": {
        "cp-files": "cp -R crawler/output server",
        "website": "yarn workspace frontend run start",
        "website:build": "yarn workspace frontend run workspace-build",
        "crawler": "yarn workspace package-crawler-backend run workspace-start",
        "server": "yarn workspace crawler-server run start-server",
        "setup-shared": "yarn workspace @nav-frontend/shared-types run transpile && yarn install",
        "dev": "yarn run setup-shared && yarn run cp-files && concurrently \"yarn run server\" \"yarn run website\""
    },
    "author": "NAV",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "frontend",
        "crawler",
        "server",
        "shared"
    ],
    "devDependencies": {
        "concurrently": "5.3.0"
    }
}
