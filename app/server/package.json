{
    "name": "crawler-server",
    "version": "0.1.0",
    "description": "Server for Package-crawler",
    "main": "server.ts",
    "license": "MIT",
    "private": true,
    "scripts": {
        "ts": "tsc",
        "start-server:dev": "ts-node-dev src/server.ts --dev",
        "start-server": "tsc && node --max-old-space-size=4092 ./build/src/server.js",
        "start-crawler-old": "tsc && node --max-old-space-size=4092 ./build/crawler/index.js",
        "start-crawler": "tsc && node ./build/crawler/index.js"
    }
}
